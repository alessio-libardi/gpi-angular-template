      "@gpi/admin/data-access/users": ["libs/admin/data-access/users/src/index.ts"],
      "@gpi/admin/entity": ["libs/admin/entity/src/index.ts"],
      "@gpi/admin/feature/public": ["libs/admin/feature/public/src/index.ts"],
      "@gpi/admin/feature/private": ["libs/admin/feature/private/src/index.ts"],
      "@gpi/admin/feature/shell": ["libs/admin/feature/shell/src/index.ts"],
      "@gpi/admin/feature/users": ["libs/admin/feature/users/src/index.ts"],
      "@gpi/admin/util/auth": ["libs/admin/util/auth/src/index.ts"],
      "@gpi/admin/util/environment": ["libs/admin/util/environment/src/index.ts"],
      "@gpi/admin/util/error": ["libs/admin/util/error/src/index.ts"],
      "@gpi/admin/util/icons": ["libs/admin/util/icons/src/index.ts"],
