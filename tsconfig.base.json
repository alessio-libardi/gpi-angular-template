      "@gpi/shared/ui/material": ["libs/shared/ui/material/src/index.ts"],
      "@gpi/admin/feature/home": ["libs/admin/feature/home/src/index.ts"],
      "@gpi/admin/feature/profile": ["libs/admin/feature/profile/src/index.ts"],
      "@gpi/admin/feature/settings": ["libs/admin/feature/settings/src/index.ts"]
